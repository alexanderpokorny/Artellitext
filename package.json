{
  "name": "artellico",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:start": "./scripts/docker-postgres.sh",
    "db:stop": "docker stop pgvector-db",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:reset": "./scripts/db.sh reset && npm run db:migrate && npm run db:seed",
    "db:psql": "./scripts/db.sh psql",
    "db:info": "./scripts/db.sh info",
    "db:backup": "./scripts/db.sh backup",
    "storage:setup": "node scripts/setup-storage.js",
    "build:prod": "./scripts/build.sh --check",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:prod": "./scripts/deploy.sh production",
    "docker:build": "docker build -t artellico:latest .",
    "docker:up": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:down": "docker-compose -f docker-compose.prod.yml down"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.49.4",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/node": "^22.10.0",
    "@types/pg": "^8.11.0",
    "svelte": "^5.20.0",
    "svelte-check": "^4.1.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.967.0",
    "@aws-sdk/s3-request-presigner": "^3.967.0",
    "@editorjs/code": "^2.9.0",
    "@editorjs/editorjs": "^2.30.0",
    "@editorjs/embed": "^2.7.0",
    "@editorjs/header": "^2.8.0",
    "@editorjs/image": "^2.9.0",
    "@editorjs/list": "^1.9.0",
    "@editorjs/paragraph": "^2.11.0",
    "@editorjs/quote": "^2.6.0",
    "@types/katex": "^0.16.8",
    "citation-js": "^0.7.22",
    "dotenv": "^17.2.3",
    "katex": "^0.16.28",
    "mermaid": "^11.12.2",
    "pg": "^8.13.0",
    "pgvector": "^0.2.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
